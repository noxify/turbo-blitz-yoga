{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "db:generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:push": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:migrate:dev": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:migrate:create": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:migrate:deploy": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^db:generate"
      ],
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ]
    },
    "lint": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "test": {}
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "DISCORD_CLIENT_ID",
    "DISCORD_CLIENT_SECRET"
  ]
}
